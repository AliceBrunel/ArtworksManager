<%= form_for @artwork do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<br>
	<%= f.label :artist_id %>
	<%= f.collection_select :artist_id, Artist.all, :id, :full_name  %>
	<br>
	<%= f.label :price %>
	<%= f.text_field :price %>
	<br>
	<%= f.label :technic %>
	<%= f.text_field :technic %>
	<br>

 <%= f.submit "Add to my collection" %>
<% end %>

