<%= form_for(:session, url: session_path) do |f| %>

 <%= f.label :name %>
 <%= f.text_field :name %>
   <br>
 <%= f.label :surname %>
 <%= f.text_field :surname %>
   <br>
 <%= f.label :email %>
 <%= f.text_field :email %>
   <br>
 <%= f.label :password %>
 <%= f.password_field :password %>
   <br>

 <%= f.label :address %>
 <%= f.text_field :address %>
   <br>
 <%= f.label :phone %>
 <%= f.text_field :phone %>
   <br>
 <%= f.label :position %>
 <%= f.text_field :position %>
 <br>
 <p> Are you a(n) :</p>
 <%= f.select(:job, @groups) %>


 <%= f.submit %>
 <% end %>