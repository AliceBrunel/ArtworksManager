<%= form_for @collection do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<br>
	<%= f.label :start_date %>
	<%= f.date_field :start_date %>
	<br>
	<%= f.label :end_date %>
	<%= f.date_field :end_date %>
	<br>
	<%= f.label :museum_id %>
	<%= f.collection_select :museum_id, Museum.all, :id, :name  %>
	<br>
	<%= f.submit "Create" %>
<% end %>

